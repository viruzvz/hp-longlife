  .container.text-center
    .row.mx-auto.my-auto.justify-content-center
      #recipeCarousel.carousel.slide.mb-5(data-bs-ride='carousel')
        .carousel-inner(role='listbox')
          .carousel-item(role="presentation").active
            .col-md-4
              .card.mb-5.border-0
                figure.cards.card-border.mb-0.cards-fix
                  img.border-radius5.u-fitCover(src="src/assets/doctors/Tiago.webp" alt='Dr. Tiago Kratka')
                  figcaption 
                    p Dr. Tiago Kratka
                    .fw-light
                      p Médico Integrativo 
                      small CRM-DF 199242
          .carousel-item(role="presentation")
            .col-md-4
              .card.mb-5.border-0
                figure.cards.card-border.mb-0.cards-fix
                  img.border-radius5.u-fitCover(src="src/assets/doctors/Ellen.webp" alt='Dra. Ellen')
                  figcaption 
                    p Dra. Ellen Saraiva
                    .fw-light
                      p Ultrossonografista
                      small CRM-DF 26764
          .carousel-item(role="presentation")
            .col-md-4
              .card.mb-5.border-0
                figure.cards.card-border.mb-0.cards-fix
                  img.border-radius5.u-fitCover(src="src/assets/doctors/Deyvid.webp" alt='Dr. Deyvid Medeiros')
                  figcaption 
                    p Dr. Deyvid Medeiros
                    .fw-light
                      p Nutricionista
                      small Clínico / Esportivo
        a.carousel-control-prev.bg-transparent.w-aut(href='#recipeCarousel' role='button' data-bs-slide='prev' title='Anterior')
          span.carousel-control-prev-icon(aria-hidden='true')
            img(src="src/assets/icons/chevron-left.svg", alt="Esquerda", width="20px")
        a.carousel-control-next.bg-transparent.w-aut(href='#recipeCarousel' role='button' data-bs-slide='next' title='Próximo')
          span.carousel-control-next-icon(aria-hidden='true')
            img(src="src/assets/icons/chevron-right.svg", alt="Direita", width="20px")
      .col-12 
        include ../components/button.pug
